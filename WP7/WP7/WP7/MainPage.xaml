							</VisualStateManager.VisualStateGroups>
							<Image x:Name="Forma_2_copia" Source="interpool_Images/Forma 2 copia.png"/>
							<Image x:Name="Forma_1_copia" Source="interpool_Images/Forma 1 copia.png" Margin="14,13,13,17"/>
